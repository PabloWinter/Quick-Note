<!-- Displaying all user notes from database in bootstrap cards -->
<div class="row">
    {{#each userNotes}}
    <div class="col-md-4">
        <div class="card mb-5" style="width: 20rem;">
            <div class="card-body">
                <h4 class="card-title">{{title}}</h4>
                <p class="card-text">{{description}}</p>
                <a href="#" class="btn btn-sm btn-primary">Edit Note</a>
                <a href="#" class="btn btn-sm btn-warning">Delete Note</a>
            </div>
        </div>
    </div>
    {{/each}}
</div>

